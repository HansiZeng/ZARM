{
    "data_dir": "dataset/toys_and_games/modified/hierachy/",
    "dataset": "Toys_and_Games_tmp",
    "log_dir": "logs",
    "log": true,
    "log_idx": 500,
    "use_pretrain": true,
    "pretrained_path": -1,
    "model_name": "relevance_cnn",
    "verbose": false,
    "stats": false,
    "stats_idx": 2,
    "parallel": false,

    "kernel_size":1,
    "hidden_dim": 100,
    "embedding_dim": 300,
    "latent_dim":32,
    "dropout": 0.5,
    "freeze_embeddings": false,
    "pooling": "AVG",
    "word_dropout": 0.2,
    "different_siamese": false,
    "mode": "user",
    "random_zero_example_rate": 0.1,
    "temperature": 0.25,
    "use_ui_bias": true,

    "epochs": 64,
    "batch_size": 64,
    "lr": 0.001,
    "reg_weight": 1.0,
    "compute_loss_repsectively": false,
    "sparse": false,
    "use_scheduler": false,
    "lr_decay": 0.5,
    "decay_patience": 0,
    "max_grad_norm": 5.0,
    "patience": 5
}